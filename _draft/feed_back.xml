---
layout: feed_back
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ site.SEOTitle }}</title>
		<description>晨曦的技术博客</description>
		<link href="https://zfb132.github.io/" rel="alternate" type="text/html"/>
		<atom:link href="https://zfb132.github.io/{{ site.RSS }}" rel="self" type="application/atom+xml" />
		{% for post in site.posts limit:10 %}
			<item>
				<title>{{ post.title }}</title>
				<content type="html" xml:base="https://zfb132.github.io{{ post.url }}">{{ post.content | xml_escape }}</content>
				<author>{{page.author}}</author>
				<pubDate>{{ post.date | date: "%a, %d %b %Y" }}</pubDate>
				<id>http://zfb132.github.io{{ post.url }}</id>
			</item>
		{% endfor %}
	</channel>
</rss>
